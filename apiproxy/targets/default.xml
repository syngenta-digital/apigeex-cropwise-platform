<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="default">
    <Description>Default target endpoint for Cropwise backend</Description>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>AM-SetTarget</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <URL>https://dev.api.insights.cropwise.com:443</URL>
        <SSLInfo>
            <Enabled>true</Enabled>
        </SSLInfo>
        <Properties>
            <Property name="connect.timeout.millis">30000</Property>
            <Property name="io.timeout.millis">60000</Property>
        </Properties>
    </HTTPTargetConnection>
</TargetEndpoint>
