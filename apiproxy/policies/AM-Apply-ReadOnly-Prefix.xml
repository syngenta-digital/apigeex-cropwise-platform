<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-Apply-ReadOnly-Prefix">
    <DisplayName>AM-Apply-ReadOnly-Prefix</DisplayName>
    <AssignVariable>
        <Name>target.copy.pathsuffix</Name>
        <Value>false</Value>
    </AssignVariable>
    <Set>
        <Path>/read-only{proxy.pathsuffix}</Path>
    </Set>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
