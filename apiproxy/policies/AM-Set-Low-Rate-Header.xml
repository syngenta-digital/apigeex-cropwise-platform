<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-Set-Low-Rate-Header">
    <DisplayName>AM-Set-Low-Rate-Header</DisplayName>
    <AssignVariable>
        <Name>rate.limit.type</Name>
        <Value>low-rate</Value>
    </AssignVariable>
    <Set>
        <Headers>
            <Header name="x-ratelimit-type">low-rate</Header>
        </Headers>
    </Set>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
