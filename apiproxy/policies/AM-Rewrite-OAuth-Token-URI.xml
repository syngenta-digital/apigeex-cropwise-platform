<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-Rewrite-OAuth-Token-URI">
    <DisplayName>AM-Rewrite-OAuth-Token-URI</DisplayName>
    <AssignVariable>
        <Name>target.copy.pathsuffix</Name>
        <Value>false</Value>
    </AssignVariable>
    <Set>
        <Path>/oauth/token</Path>
    </Set>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
