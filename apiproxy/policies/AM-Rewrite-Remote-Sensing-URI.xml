<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-Rewrite-Remote-Sensing-URI">
    <DisplayName>AM-Rewrite-Remote-Sensing-URI</DisplayName>
    <AssignVariable>
        <Name>target.copy.pathsuffix</Name>
        <Value>false</Value>
    </AssignVariable>
    <Set>
        <Path>/remote-sensing/api{proxy.pathsuffix}</Path>
    </Set>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
