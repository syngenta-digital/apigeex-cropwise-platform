<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="EV-Extract-JWT-Token">
    <DisplayName>EV-Extract-JWT-Token</DisplayName>
    <Source>request</Source>
    <Header name="Authorization">
        <Pattern ignoreCase="true">Bearer {token}</Pattern>
    </Header>
    <VariablePrefix>jwt</VariablePrefix>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</ExtractVariables>
